{
    "name": "@frontend/wknd-components",
    "version": "42.9.0",
    "description": "Repo for all `wknd-components` used in both `wknd-storybook` and in the `wknd-platform` applications.",
    "main": "dist/index.js",
    "types": "dist/index.js",
    "engines": {
        "npm": ">=7",
        "node": ">=14"
    },
    "scripts": {
        "start": "npm run storybook dev",
        "test": "NODE_ENV=test jest",
        "test:watch": "NODE_ENV=test jest --watch",
        "test:ci": "NODE_ENV=test jest --ci --coverage",
        "clean": "rimraf dist && rimraf storybook-static",
        "prebuild": "npm run clean",
        "build": "run-s build:*",
        "predevelop": "npm run clean",
        "publish:dry-run": "npm publish --dry-run",
        "develop": "npx tsc --project ./tsconfig.develop.json -w",
        "prepare": "husky install",
        "lint": "eslint -c .eslintrc src/**",
        "lint:ci": "run-s lint:ci:*",
        "lint:ci:junit": "eslint -c .eslintrc -f junit -o ./reports/junit/js-lint-results.xml **/*.{ts,tsx}",
        "lint:ci:json": "eslint -c .eslintrc -f json -o ./reports/json/js-lint-results.json **/*.{ts,tsx}",
        "update-version": "standard-version",
        "update-version:minor": "standard-version --release-as minor",
        "update-version:patch": "standard-version --release-as patch",
        "update-version:major": "standard-version --release-as major",
        "storybook": "storybook dev -p 6006",
        "build:storybook": "storybook build",
        "build:tsc": "npx tsc --build ./tsconfig.prod.json",
        "build:package": "npm pack"
    },
    "overrides": {
        "semver": "^7.5.3"
    },
    "publishConfig": {
        "@frontend:registry": "https://gitlab.bouncex.net/api/v4/projects/730/packages/npm/"
    },
    "files": [
        "dist/**",
        ".npmrc"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@radix-ui/react-dropdown-menu": "^2.0.1",
        "@radix-ui/react-popover": "^1.0.2",
        "@radix-ui/react-tabs": "^1.0.1",
        "@types/react-modal": "^3.13.1",
        "attr-accept": "^2.2.2",
        "date-fns": "^2.30.0",
        "dayzed": "^3.2.3",
        "lucide-react": "^0.400.0",
        "react-data-table-component": "^7.5.3",
        "react-grid-system": "^8.1.9",
        "react-modal": "^3.15.1",
        "react-select": "^5.5.9",
        "styled-components": "^5.3.1"
    },
    "devDependencies": {
        "@babel/core": "^7.23.6",
        "@babel/plugin-proposal-export-default-from": "^7.22.5",
        "@babel/plugin-transform-modules-commonjs": "^7.22.5",
        "@babel/plugin-transform-runtime": "^7.15.0",
        "@babel/preset-env": "^7.22.9",
        "@babel/preset-react": "^7.22.5",
        "@babel/preset-typescript": "^7.22.5",
        "@commitlint/cli": "^17.1.2",
        "@commitlint/config-conventional": "^17.1.0",
        "@date-fns/tz": "^1.2.0",
        "@etchteam/storybook-addon-status": "^4.2.4",
        "@storybook/addon-a11y": "^7.6.6",
        "@storybook/addon-actions": "^7.6.6",
        "@storybook/addon-controls": "^7.6.6",
        "@storybook/addon-docs": "^7.6.6",
        "@storybook/addon-essentials": "^7.6.6",
        "@storybook/addon-interactions": "^7.6.6",
        "@storybook/addon-links": "^7.6.6",
        "@storybook/blocks": "^7.6.6",
        "@storybook/client-api": "^7.6.6",
        "@storybook/react": "^7.6.6",
        "@storybook/react-webpack5": "^7.6.6",
        "@storybook/testing-library": "^0.2.2",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest": "^29.0.0",
        "@types/node": "^18.0.0",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.0.9",
        "@types/styled-components": "^5.1.12",
        "@types/testing-library__jest-dom": "^5.14.1",
        "@typescript-eslint/eslint-plugin": "^5.59.1",
        "@typescript-eslint/parser": "^5.59.1",
        "babel-jest": "^29.0.0",
        "dompurify": "^3.0.5",
        "eslint": "^8.57.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-plugin-disable": "^2.0.3",
        "eslint-plugin-import": "^2.24.0",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "eslint-plugin-storybook": "^0.6.15",
        "eslint-plugin-testing-library": "^7.0.0",
        "file-loader": "^6.2.0",
        "husky": "^8.0.0",
        "jest": "^29.0.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-junit": "^15.0.0",
        "lint-staged": "^13.1.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.3.2",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-docgen-typescript": "^2.2.2",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.43.8",
        "react-router-dom": "^6.14.2",
        "react-select-event": "^5.5.1",
        "recharts": "^2.10.1",
        "rimraf": "^4.0.0",
        "standard-version": "^9.5.0",
        "storybook": "^7.6.6",
        "ts-jest": "^29.0.0",
        "typescript": "^5.2.2"
    },
    "peerDependencies": {
        "react": "^16.13.1 || ^17.0.2 || ^18.2.0",
        "react-dom": "^16.13.1 || ^17.0.2 || ^18.2.0"
    },
    "lint-staged": {
        "*.{ts,tsx,js,tsx}": "eslint --cache --fix",
        "*.{ts,tsx,js,css,md}": "prettier --write"
    },
    "jest-junit": {
        "outputDirectory": "./reports/junit",
        "outputName": "unit-test-results.xml"
    }
}
